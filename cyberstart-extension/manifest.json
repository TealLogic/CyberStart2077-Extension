{
  "manifest_version": 3,
  "name": "Cyberpunk 2077 Themed Homepage",
  "version": "1.0",
  "description": "A Cyberpunk 2077 themed homepage.",
  "action": {
    "default_title": "Go to CyberStart",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "support@teallogic.com",
      "strict_min_version": "102.0"
    }
  }
}
