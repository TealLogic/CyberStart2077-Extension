{
  "manifest_version": 2,
  "name": "Cyberpunk 2077 Themed Homepage",
  "version": "1.1",
  "description": "A Cyberpunk 2077 themed homepage.",
  "browser_action": {
    "default_title": "Go to CyberStart",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "support@teallogic.com",
      "strict_min_version": "102.0"
    }
  }
}
